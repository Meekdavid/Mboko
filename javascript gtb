let GTStaff = {
  worksAt: "GtBank",
  branch: "Head Office",
  favoriteColor: "Orange",
  getBranch: function () {
    alert(this.branch);
  }
};

let Damola = {
  __proto__: GTStaff
};

let Deji = {
  __proto__: GTStaff
};

console.log("Damola's Branch:", Damola.branch);
console.log("Deji's Branch:", Deji.branch);


let Person = { talk: "Halleluyah", hairColor: "brown", speakUp: function () { alert(this.talk); }, showParentObject: function () { console.log(this); }};
let Oyinda = { talk: "Hossana", __proto__: Person};
console.log(Oyinda.);
-- 
GMNSE, MIAENGDelta, Nigeria.


function YorubaPerson(state) {
  this.state = state;
  this.facialFeatures = "Tribal marks";
  this.favouriteFood = "Amala & Gbegiri";
  this.favouritSpice = "Pepper";
  
}

class IgboPerson {
  constructor(state) {
    this.state = state;
    this.favouriteActivity = "Business";
    this.favouriteThing = "Money";
  }
}

let Damola = new YorubaPerson("Osun");
let Chinedu = new IgboPerson("Anambra");
console.log(Chinedu.favouriteActivity);
