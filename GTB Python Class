name = 'Alice'
age = 25
if name =='Alice':
    print('Hi Alice.')
elif age<12:
    print('You are not Alice, Kiddo.')
elif age>2000:
    print('Unlike you, Alice is not an undead, immortal vampire')
elif age<100:
    print('You are not Alice, Girl.')
else:
    print('No condition met')
    
    
    
    name = 'lice'
age = 9
if name =='Alice':
    print('Hi Alice.')
elif age<12 and name=='lice':
    print('You are not Alice, Kiddo.')
elif age<2000:
    print('Unlike you, Alice is not an undead, immortal vampire')
elif age<100:
    print('You are not Alice, Girl.')
else:
    print('No condition met')
